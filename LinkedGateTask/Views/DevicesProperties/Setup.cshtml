
@{
    ViewBag.Title = "Setup";
}
@foreach (var item in (List<DataManager.Models.Views.DevCatProps>)Model)
{
    <div class="form-group">
        @Html.Label(item.Description, htmlAttributes: new { @class = "control-label col-md-2" })
        <div class="col-md-10">
            @Html.Editor("devProp_" + item.PropId, new { htmlAttributes = new { @class = "form-control", @Value = item.Value,dataValue = item.DevPropId } })

        </div>

    </div>
}

